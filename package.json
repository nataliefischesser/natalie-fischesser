    "@types/node": "~20.10.2",
    "@unocss/eslint-config": "^0.57.7",
    "@unocss/nuxt": "^0.57.7",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vueuse/motion": "^2.0.0",
    "@vueuse/nuxt": "^10.6.1",
    "eslint": "^8.55.0",
